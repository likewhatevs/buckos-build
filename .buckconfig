[cells]
  buckos = .
  prelude = prelude
  toolchains = toolchains

[cell_aliases]
  config = prelude
  buck = buckos

[external_cells]
  prelude = bundled

[build]
  default_target_platform = //platforms:linux-x86_64
  execution_platforms = //tc/exec:platforms

[parser]
  target_platform_detector_spec = target:buckos//...->buckos//platforms:linux-x86_64 target:toolchains//...->buckos//platforms:linux-x86_64

[project]
  ignore = .git, buck-out, **/__pycache__, .claude

[buckos]
  patch_registry_enabled = true
  default_toolchain = buckos//tc/seed:seed-toolchain

[mirror]
  mode = upstream
